								IS_CLOUDFLARE_TURNSTILE_ENABLED = isCloudflareTurnstileEnabled;
							}
						};
						
						function appendMessageAndConditionallyScroll(chatMessageContent, liveChatUsername, liveChatUsernameColorCode, sentTimestamp) {
							const chatMessagesContainer = $("#chat-messages-container");
							const chatMessages = $("#chat-messages");

							const chatMessageElement = $('<div class="chat-message pb-2"><div class="flex-shrink-1 rounded px-2"><b style="color:#' + liveChatUsernameColorCode + ';font-size:0.8em;">' + liveChatUsername + '</b>: <span class="live-chat-text-color">' + chatMessageContent + '</span></div></div>');
							
